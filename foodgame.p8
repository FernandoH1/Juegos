function init_game()
	cls(4)
	print("game üÖæÔ∏èver!!",40,60)
	print("press ‚ùé to play again",20,70)
end

function update_menu()
	--sfx(05)
	if btnp(‚ùé) then
		_update = _update_juego
		_draw = _draw_juego
	end
	
	if btnp(‚¨ÜÔ∏è) then
		_update = update_i
		_draw = init_i
	end
end

function update_gameover()
	 if btnp(‚ùé) then
	 _update = _update_juego
		_draw = _draw_juego
		puntos=0
		nivel=0
		frutas={} 
	end
end

function update_i()
	--sfx(05)
	if btnp(‚¨ÜÔ∏è) then
		_update = update_menu
		_draw = init_menu 
	end
end
